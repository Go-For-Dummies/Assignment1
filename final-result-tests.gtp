# NoGo test for checking the final result works 
# 'draw' is not a possible result in the rules of NoGo

boardsize 2
clear_board

00 gogui-rules_final_result
#? [unknown]

play b a1 

10 gogui-rules_final_result
#? [unknown]

play w a2 

20 gogui-rules_final_result
#? [unknown]

play b b1

30 gogui-rules_final_result
#? [black]
# black wins, whites final move would be a capture

boardsize 3
clear_board

40 gogui-rules_final_result
#? [unknown]
# game is not finished

play b a1
play w a2
play b b1
play w b2
play b c1
play w a3
play b b3

50 gogui-rules_final_result
#? [black]
# black wins, whites final move would be a capture

clear_board

play b a1
play w a2
play b b2
play w a3
play b b1
play w b3
play b c1
play w c3

60 gogui-rules_final_result
#? [white]
# white wins, blacks final move would be a capture

boardsize 4
clear_board

play b a2 
play w d1 
play b b1 
play w d2 
play b b2 
play w d3
play b b3 
play w d4
play b c2
play w a3
play b c3 
play w a4
play b c4

70 gogui-rules_final_result 
#? [black] 
# black wins, all final moves are suicide for white
